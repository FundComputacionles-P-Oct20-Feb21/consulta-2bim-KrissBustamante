Algoritmo calculo_multiplicacion
	Definir a , z ,b Como Entero;
	Escribir 'Ingrese el primer numero para multiplicar' Sin Saltar;
	Leer a;
	Escribir 'Ingrese el segundo numero para multiplicar'Sin Saltar;
	Leer b 
	Escribir 'Ingrese el tercer numero para multiplicar'Sin Saltar;
	Leer z 
	resultado <-- a*b*z;
	Escribir "El resultado de la multiplicacion es:";
	Escribir resultado;
FinAlgoritmo
